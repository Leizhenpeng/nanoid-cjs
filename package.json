{
  "name": "nanoid-cjs",
  "type": "module",
  "version": "0.0.5",
  "packageManager": "pnpm@9.1.1",
  "description": "latest nanoid, but support cjs for nodejs",
  "author": "leizhenpeng <laolei@forkway.cn>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/leizhenpeng",
  "homepage": "https://github.com/leizhenpeng/nanoid-cjs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leizhenpeng/nanoid-cjs.git"
  },
  "bugs": "https://github.com/leizhenpeng/nanoid-cjs/issues",
  "keywords": [
    "nanoid",
    "cjs",
    "node"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "ncc build src/index.ts -o dist",
    "dts": "tsc --emitDeclarationOnly",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "nanoid": "^5.0.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.1",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.8",
    "@types/node": "^20.12.12",
    "@vercel/ncc": "^0.38.1",
    "bumpp": "^9.4.1",
    "eslint": "^9.3.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^9.1.1",
    "rimraf": "^5.0.7",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
